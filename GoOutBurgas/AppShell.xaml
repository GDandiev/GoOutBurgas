<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="GoOutBurgas.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:GoOutBurgas.Pages"
    Shell.TabBarIsVisible="False"
    Shell.BackgroundColor="{AppThemeBinding Light = #2b62aa, Dark=#2f3889}"
    TabBarBackgroundColor="{AppThemeBinding Light=#2b62aa, Dark=#2f3889}"
    Shell.TabBarUnselectedColor="{AppThemeBinding Light=White, Dark=White}" Shell.TabBarTitleColor="{AppThemeBinding Light= #2b62aa, Dark= White}"
    Shell.TabBarDisabledColor="#252525">
    
    <TabBar>
        
        <Tab Title="Map" Icon="{AppThemeBinding Light=thisisformaps.png, Dark=thisisformapsdark.png}">
            
            <ShellContent ContentTemplate="{DataTemplate local:LoginPage}" />
            
        </Tab>
         
    </TabBar>

</Shell>
